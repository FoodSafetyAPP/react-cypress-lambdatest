pipeline {
  agent {
    docker {
      image 'cypress/base:16.14.0'
    }
  }

  environment {
    CYPRESS_PROJECT_ID = credentials('CYPRESS_PROJECT_ID')
    CYPRESS_RECORD_KEY = credentials('CYPRESS_RECORD_KEY')
  }

  stages {
    stage('verify installation') {
      steps {
        sh 'cat /etc/lsb-release'
      }
    }
  }
}