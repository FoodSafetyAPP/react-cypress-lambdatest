pipeline {
  agent any

  environment {
    LT_USERNAME = credentials('LT_USERNAME')
    LT_ACCESS_KEY = credentials('LT_ACCESS_KEY')
  }

  stages {
    

    stage('execute E2E') {
      steps {
        sh "sudo npm i -g lambdatest-cypress-cli"
      }
      steps {
        sh "sudo npm uninstall -g lambdatest-cypress-cli"
      }
    }
  }
}
