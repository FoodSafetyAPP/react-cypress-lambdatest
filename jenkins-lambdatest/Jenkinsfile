def myVariable = "dsds"
pipeline {
  agent any

  environment {
    LT_USERNAME = credentials('LT_USERNAME')
    LT_ACCESS_KEY = credentials('LT_ACCESS_KEY')
  }

  stages {

    stage('execute E2E') {
      steps {
        script{
          myVariable = sh(returnStdout: true, script: 'lambdatest-cypress run --eof true --bn=hhdh').trim()
        }
        echo "output-${myVariable}"
      }
    }
  }
}