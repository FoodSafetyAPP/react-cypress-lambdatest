{
  "name": "demoapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint ./src",
    "ci:lambdatest": "start-server-and-test start-server 3000 lambdatest-cypress:run",
    "start-server": "http-server ./build -p 3000",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:verify": "cypress verify",
    "cy:info": "cypress info",
    "cy:version": "cypress version",
    "cy:cache:list": "cypress cache list",
    "e2e:record": "cypress run --record --group e2e-specs-cypress --ci-build-id $npm_config_id-$npm_config_name",
    "e2e:record:parallel": "cypress run --record --group e2e-specs-cypress-parallel --ci-build-id $npm_config_id-$npm_config_name --parallel",
    "e2e:chrome": "cypress run --browser chrome",
    "e2e:firefox": "cypress run --browser firefox",
    "e2e:edge": "cypress run --browser edge",
    "e2e:record:chrome": "cypress run --record --browser chrome",
    "e2e:record:firefox": "cypress run --record --browser firefox",
    "e2e:record:edge": "cypress run --record --browser edge",
    "lambdatest-cypress:run": "npx lambdatest-cypress run --eof=true --bn=$npm_config_build_name"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "airbnb",
      "plugin:react/jsx-runtime"
    ],
    "rules": {
      "comma-dangle": [
        "error",
        "never"
      ],
      "arrow-parens": [
        "error",
        "as-needed"
      ],
      "object-curly-newline": [
        "error",
        "never"
      ]
    }
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cypress": "^9.5.2",
    "eslint-config-airbnb": "^19.0.4",
    "http-server": "^14.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "serve": "^13.0.2",
    "start-server-and-test": "^1.14.0"
  }
}